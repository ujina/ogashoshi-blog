{ posts, path } = this._metaMap[name]\n      tags.push({ name, posts, path })\n    })\n    return tags\n  }\n\n  getItemByName (name) {\n    return this._metaMap[name]\n  }\n}\n\nexport default ({ Vue }) => {\n  Vue.mixin({\n    computed: {\n      $tags () {\n        const { pages } = this.$site\n        const tags = new Classifiable(tagMeta, pages)\n        return tags\n      },\n      $tag () {\n        const tagName = this.$route.meta.tagName\n        return this.$tags.getItemByName(tagName)\n      },\n      $categories () {\n        const { pages } = this.$site\n        const categories = new Classifiable(categoryMeta, pages)\n        return categories\n      },\n      $category () {\n        const categoryName = this.$route.meta.categoryName\n        return this.$categories.getItemByName(categoryName)\n      },\n    },\n  })\n}\n"],"mappings":"AAAA;AAEA,SAASA,aAAT,QAA8B,WAA9B;AACA,OAAOC,OAAP,MAAoB,cAApB;AACA,OAAOC,YAAP,MAAyB,mBAAzB;;AAEA,MAAMC,YAAN,CAAmB;EACjBC,WAAW,CAAEC,OAAF,EAAWC,KAAX,EAAkB;IAC3B,KAAKC,QAAL,GAAgBC,MAAM,CAACC,MAAP,CAAc,EAAd,EAAkBJ,OAAlB,CAAhB;IACAG,MAAM,CAACE,IAAP,CAAY,KAAKH,QAAjB,EAA2BI,OAA3B,CAAmCC,IAAI,IAAI;MACzC,MAAM;QAAEC;MAAF,IAAe,KAAKN,QAAL,CAAcK,IAAd,CAArB;MACA,KAAKL,QAAL,CAAcK,IAAd,EAAoBE,KAApB,GAA4BD,QAAQ,CAACE,GAAT,CAAaC,GAAG,IAAIhB,aAAa,CAACM,KAAD,EAAQU,GAAR,CAAjC,CAA5B;IACD,CAHD;EAID;;EAES,IAANC,MAAM,GAAI;IACZ,OAAOT,MAAM,CAACE,IAAP,CAAY,KAAKH,QAAjB,EAA2BU,MAAlC;EACD;;EAEM,IAAHF,GAAG,GAAI;IACT,OAAO,KAAKR,QAAZ;EACD;;EAEO,IAAJW,IAAI,GAAI;IACV,OAAO,KAAKC,OAAL,EAAP;EACD;;EAEDA,OAAO,GAAI;IACT,MAAMC,IAAI,GAAG,EAAb;IACAZ,MAAM,CAACE,IAAP,CAAY,KAAKH,QAAjB,EAA2BI,OAA3B,CAAmCC,IAAI,IAAI;MACzC,MAAM;QAAEE,KAAF;QAASO;MAAT,IAAkB,KAAKd,QAAL,CAAcK,IAAd,CAAxB;MACAQ,IAAI,CAACE,IAAL,CAAU;QAAEV,IAAF;QAAQE,KAAR;QAAeO;MAAf,CAAV;IACD,CAHD;IAIA,OAAOD,IAAP;EACD;;EAEDG,aAAa,CAAEX,IAAF,EAAQ;IACnB,OAAO,KAAKL,QAAL,CAAcK,IAAd,CAAP;EACD;;AAhCgB;;AAmCnB,gBAAe,CAAC;EAAEY;AAAF,CAAD,KAAa;EAC1BA,GAAG,CAACC,KAAJ,CAAU;IACRC,QAAQ,EAAE;MACRC,KAAK,GAAI;QACP,MAAM;UAAErB;QA